{
    "swagger": "2.0",
    "info": {
        "title": "Laravel Vue",
        "version": "1.0.0"
    },
    "basePath": "/api",
    "paths": {
        "/products": {
            "get": {
                "tags": [
                    "Products"
                ],
                "summary": "List Products",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "406": {
                        "description": "not acceptable"
                    },
                    "500": {
                        "description": "internal server error"
                    }
                }
            }
        },
        "/products/{product}/reviews": {
            "post": {
                "tags": [
                    "Products"
                ],
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "customer",
                        "in": "formData",
                        "description": "Customer Name",
                        "type": "string"
                    },
                    {
                        "name": "star",
                        "in": "formData",
                        "description": "Rating Rating",
                        "type": "string"
                    },
                    {
                        "name": "review",
                        "in": "formData",
                        "description": "Reveiw",
                        "type": "string"
                    },
                    {
                        "name": "product",
                        "in": "path",
                        "description": "Product Id",
                        "type": "integer"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Review Create Create"
                    }
                }
            }
        }
    },
    "definitions": {}
}